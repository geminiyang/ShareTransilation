<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:yqq="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_three"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.yqq.touchtest.Activity.ThreeActivity">
    <!--轮播图-->
    <android.support.v4.view.ViewPager
        android:id="@+id/viewpager_banner"
        android:layout_width="match_parent"
        android:layout_height="225dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentStart="true">
    </android.support.v4.view.ViewPager>
    <!--指示器-->
    <com.yqq.touchtest.MyWidget.IndicatorView
        android:id="@+id/indicatorview_banner"
        android:layout_width="match_parent"
        android:layout_height="20dp"
        yqq:indicatorInterval="10dp"
        yqq:normalDrawable="@mipmap/white"
        yqq:selectDrawable="@mipmap/green"
        android:layout_marginTop="-20dp"
        android:layout_below="@+id/viewpager_banner"
        android:gravity="center_horizontal"
        yqq:isCirculate="true"
        >
    </com.yqq.touchtest.MyWidget.IndicatorView>


    <com.yqq.touchtest.MyWidget.CircleView
        android:id="@+id/circleview"
        android:layout_width="230dp"
        android:layout_height="230dp"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="80dp"
        android:layout_centerHorizontal="true"
        />


</RelativeLayout>
